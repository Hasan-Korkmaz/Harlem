    <div class="cont with-social"
         style=" background: url('/Backoffice/Images/General/Background/LoginBackground.jpg') center center / cover no-repeat; ">
        <div class="giris">
            <div class="login" >
                <div class="logo" style="display:flex;justify-content:space-between">
                    <a href="/" style="width: 100%; display: flex; justify-content:center; text-decoration: none; color: black; ">
                        <span style="display: flex; justify-content: center; align-items: center; font-size: 85px; font-family: 'Libre Barcode 39 Text', cursive;">
                            HARLEM
                        </span>
                    </a>

                </div>
                <form method="post" action="\backoffice\account\login" name="loginForm" role="form">
                    <div>
                        <div class="providerLinkingFeedback" style="z-index: 15"></div>
                        <div class="baslik">YÖNETİCİ GİRİŞİ</div>
                    </div>
                    <div class="login__form">
                        <div class="login__row">
                            <svg class="login__icon name svg-icon" viewBox="0 0 20 20">
                                <path d="M0,20 a10,8 0 0,1 20,0z M10,0 a4,4 0 0,1 0,8 a4,4 0 0,1 0,-8"></path>
                            </svg>
                            <input class="login__input name form-control"
                                   type="text"
                                   name="email"
                                   id="inputEmail"
                                   placeholder="Kullanıcı adınızı giriniz."
                                   autofocus="" />
                        </div>
                        <div class="login__row">
                            <svg class="login__icon pass svg-icon" viewBox="0 0 20 20">
                                <path d="M0,20 20,20 20,8 0,8z M10,13 10,16z M4,8 a6,8 0 0,1 12,0"></path>
                            </svg>
                            <input type="password"
                                   name="password"
                                   id="inputPassword"
                                   class="login__input pass form-control"
                                   placeholder="Şifrenizi giriniz"
                                   autocomplete="off" />
                        </div>
                        <div>
                            <label>
                                <input type="checkbox" name="RememberMe" value=true /> Oturumumu açık
                                bırak
                            </label>
                        </div>
                        <button type="submit" class="login__submit">Oturum Aç</button>

                    </div>
                    <div style="display: flex; justify-content: center; margin-top: 350px;">
                        @if (!String.IsNullOrEmpty(ViewBag.AuthMessage))
                        {
                            <div class="login-error">@ViewBag.AuthMessage</div>

                        }
                    </div>
                </form>
            </div>
        </div>
    </div>
    <script>
    $(document).ready(function () {
    var animating = false,
    submitPhase1 = 1100,
    submitPhase2 = 400,
    logoutPhase1 = 800,
    $login = $(".login"),
        $girisform = document.getElementsByName("loginForm")[0];
    $app = $(".app");

    function ripple(elem, e) {
    $(".ripple").remove();
    var elTop = elem.offset().top,
    elLeft = elem.offset().left,
    x = e.pageX - elLeft,
    y = e.pageY - elTop;
    var $ripple = $("<div class='ripple'></div>");
    $ripple.css({ top: y, left: x });
    elem.append($ripple);
    }

    $(document).on("click", ".login__submit", function (e) {
    if (animating) return;
    animating = true;
    var that = this;
    ripple($(that), e);
    $girisform.submit();
    $(that).addClass("processing");
    setTimeout(function () {
    $(that).addClass("success");
    setTimeout(function () {
    $app.show();
    $app.css("top");
    $app.addClass("active");
    }, submitPhase2 - 70);
    setTimeout(function () {
    $login.hide();
    $login.addClass("inactive");
    animating = false;
    $(that).removeClass("success processing");
    }, submitPhase2);
    }, submitPhase1);
    });

    $(document).on("click", ".app__logout", function (e) {
    if (animating) return;
    $(".ripple").remove();
    animating = true;
    var that = this;
    $(that).addClass("clicked");
    setTimeout(function () {
    $app.removeClass("active");
    $login.show();
    $login.css("top");
    $login.removeClass("inactive");
    }, logoutPhase1 - 120);
    setTimeout(function () {
    $app.hide();
    animating = false;
    $(that).removeClass("clicked");
    }, logoutPhase1);
    });
    });</script>
